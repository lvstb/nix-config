{
  "placeholder": "Search applications...",
  "keep_open": false,
  "enable_typeahead": true,
  "show_initial_entries": true,
  "terminal": "ghostty",
  "orientation": "vertical",
  "fullscreen": false,
  "width": 600,
  "height": 500,
  "theme": {
    "style_sheet": "style.css"
  },
  "modules": [
    {
      "name": "applications",
      "prefix": "",
      "switcher_exclusive": false
    },
    {
      "name": "runner",
      "prefix": "!",
      "switcher_exclusive": false
    },
    {
      "name": "ssh",
      "prefix": "ssh ",
      "switcher_exclusive": false,
      "history": true
    },
    {
      "name": "finder",
      "prefix": "~",
      "switcher_exclusive": false,
      "file_manager": "nautilus"
    },
    {
      "name": "commands",
      "prefix": "/",
      "switcher_exclusive": false
    },
    {
      "name": "websearch",
      "prefix": "?",
      "switcher_exclusive": false,
      "engines": [
        {
          "name": "Google",
          "url": "https://www.google.com/search?q=%s"
        },
        {
          "name": "DuckDuckGo", 
          "url": "https://duckduckgo.com/?q=%s"
        },
        {
          "name": "GitHub",
          "url": "https://github.com/search?q=%s"
        }
      ]
    },
    {
      "name": "clipboard",
      "prefix": "clip ",
      "switcher_exclusive": false
    },
    {
      "name": "windows",
      "prefix": "win ",
      "switcher_exclusive": false
    }
  ],
  "external": [
    {
      "name": "calculator",
      "prefix": "=",
      "src": "qalc"
    }
  ],
  "icons": {
    "theme": "Papirus-Dark",
    "size": 32
  },
  "list": {
    "max_entries": 8,
    "single_click": false,
    "margin_top": 10,
    "margin_bottom": 10
  },
  "clipboard": {
    "max_entries": 20,
    "image_height": 200
  },
  "search": {
    "delay": 100,
    "hide_icons": false,
    "margin": 10,
    "hide_spinner": false
  },
  "runner": {
    "excludes": [
      ".git",
      ".svn",
      "node_modules",
      "__pycache__"
    ]
  },
  "activation_mode": {
    "disabled": false,
    "use_f_keys": false,
    "use_alt": false,
    "use_ctrl": false
  },
  "keybindings": {
    "clear": "Ctrl+l",
    "select_next": ["Down", "Tab"],
    "select_prev": ["Up", "Shift+Tab"],
    "submit": "Return",
    "quit": "Escape",
    "activate_module": {
      "applications": "Alt+a",
      "runner": "Alt+r",
      "ssh": "Alt+s",
      "finder": "Alt+f",
      "websearch": "Alt+w",
      "clipboard": "Alt+c",
      "windows": "Alt+Tab"
    }
  }
}