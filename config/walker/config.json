{
  "placeholder": "Search...",
  "keep_open": false,
  "enable_typeahead": true,
  "show_initial_entries": true,
  "terminal": "ghostty",
  "orientation": "vertical",
  "fullscreen": false,
  "width": 600,
  "height": 400,
  "theme": {
    "style_sheet": "style.css"
  },
  "modules": [
    {
      "name": "applications",
      "prefix": "",
      "switcher_exclusive": false
    },
    {
      "name": "runner",
      "prefix": "!",
      "switcher_exclusive": false
    },
    {
      "name": "ssh",
      "prefix": "ssh ",
      "switcher_exclusive": false
    },
    {
      "name": "finder",
      "prefix": "~",
      "switcher_exclusive": false
    },
    {
      "name": "commands",
      "prefix": "/",
      "switcher_exclusive": false
    },
    {
      "name": "websearch",
      "prefix": "?",
      "switcher_exclusive": false
    }
  ],
  "external": [
    {
      "name": "calculator",
      "prefix": "=",
      "src": "walker-calc"
    }
  ],
  "icons": {
    "theme": "Adwaita",
    "size": 24
  },
  "list": {
    "max_entries": 10,
    "single_click": false
  },
  "clipboard": {
    "max_entries": 10,
    "image_height": 100
  },
  "search": {
    "delay": 100,
    "hide_icons": false,
    "margin": 10,
    "hide_spinner": false
  },
  "runner": {
    "excludes": [
      ".git",
      ".svn"
    ]
  },
  "activation_mode": {
    "disabled": false,
    "use_f_keys": false,
    "use_alt": false,
    "use_ctrl": false
  }
}